
{
  "manifest_version": 2,
  "name": "count links => browser action",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "background": {
    "scripts": ["event.js"],
    "parsistent": false
  },

  "browser_action": {
    "default_icon": {
      "16": "someicon.png"
    }
  }
}

